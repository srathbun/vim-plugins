		/* ${2:notes} */

		schema: {

			$6: {
				type: ${7:String},
				description: "$8",
				isRequired: ${9:true},
			}

		},

		constructor: function $1(){
			${3://Do nothing}
		},

		statics: {
			${4://No static vars}
		},

		members: {

			${5://snipmate object method uses :f}

		}

	});
# make stack trace
snippet makeStack
	var e = new Error('dummy');
	var stack = e.stack.replace(/^[^\(]+?[\n$]/gm, '')
		.replace(/^\s+at\s+/gm, '')
		.replace(/^Object.<anonymous>\s*\(/gm, '{anonymous}()@')
		.split('\n');
	console.log(stack);
